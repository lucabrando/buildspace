{
    "version": 2,
    "functions": {
        "api/*.py": {
          "runtime": "@vercel/python"
        }
      },
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }