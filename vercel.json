{
    "version": 2,
    "functions": {
        "api/*.py": {
            "excludeFiles": "{**/.venv,**/.env,**/*.pyc}",
            "runtime": "python3.9"
        }
    },
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}